<Application
    x:Class="ContactsDemo.App.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="using:ContactsDemo.App.Converters"
    xmlns:local="using:ContactsDemo.App">
    <Application.Resources>
        <ResourceDictionary>
            <converters:StreamToImageSourceConverter x:Key="StreamToImageSourceConverter" />
            <converters:InverseBooleanConverter x:Key="InverseBooleanConverter" />
            <converters:IsNullToBooleanConverter x:Key="IsNullToBooleanConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
